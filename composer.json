{
    "name": "mrweb/laravel-down-asap",
    "description": "Put Laravel in maintenance mode once the DB is idle",
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/MrWeb/laravel-down-asap"       
    }],
    "autoload": {
        "psr-4": {
            "Mrweb\\DownAsap\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrweb\\DownAsap\\DownAsapServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/framework": "^6.0 || ^7.0"
    },
    "minimum-stability": "1.0.2"
}
